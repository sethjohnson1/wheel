				//$('.blog-loading').hide();
				$('.contentRow-content').append('<div class="ajaxContent hidden Spring_planting"><h3>'+data.title+'</h3>'+data.content+'</div>');
				
			},
			complete: function(){
				$('.video-container').addClass('youtube-container');
				//definitely a flaw in this logic, since all lightbox data-rel will be cascaded with ajaxPic_class....
				$('[data-rel^="lightbox"]').addClass('ajaxPic_40546');
				initColorBox('40546');
				
				//download the attachments
				$(document).find('[class^="wp-image-"]').each(function( index,element ) {
					console.log($(this).attr('src'));
				});
			},
			url:"https://centerofthewest.org/wp-json/posts/40571/?_jsonp=?"
		});
		//return false;
		//});
	
				//$(document).ready(function() { 
		$.ajax({
			async:true,
			dataType:"jsonp",
			success:function (data, textStatus) {
				//console.log(data);
				$('.wp-title').text(data.title);
				//$('.wp-author').text('By '+data.author.name);
				//$('.wp-content').html(data.content);
				originalContent=data.content;
				//$('.blog-loading').hide();
				$('.contentRow-content').append('<div class="ajaxContent hidden Spring_gathering_willows"><h3>'+data.title+'</h3>'+data.content+'</div>');
				
			},
			complete: function(){
				$('.video-container').addClass('youtube-container');
				//definitely a flaw in this logic, since all lightbox data-rel will be cascaded with ajaxPic_class....
				$('[data-rel^="lightbox"]').addClass('ajaxPic_40546');
				initColorBox('40546');
				
				//download the attachments
				$(document).find('[class^="wp-image-"]').each(function( index,element ) {
					console.log($(this).attr('src'));
				});
			},
			url:"https://centerofthewest.org/wp-json/posts/40572/?_jsonp=?"
		});
		//return false;
		//});
	
